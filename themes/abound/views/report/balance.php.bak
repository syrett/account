function echoData($key, $name="default",$css="table-c")
{

  if (empty($data[$key]))
    {
      echo "<div class=".$css.">";
      echo "<th>".$name."</th>";
      echo "<td> 0 </td>";
      echo "<td> 0 </td>";
      echo "</div>";
    }
  else
    {
      $arr=$data[$key];
      echo "<div class=".$css.">";
      if($name=="default")
        echo "<th>".$arr["name"]."</th>";
      else
        echo "<th>".$name."</th>";
      echo "<td>".$arr["start"]." </td>";
      echo "<td>" .$arr["end"]."</td>";
      echo "</div>";
    }
}
